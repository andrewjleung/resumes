{
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.rulers": [
            120
        ]
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".*\\.ts",
                "cmd": "bun run ${workspaceFolder}/render.ts"
            },
        ]
    },
}
